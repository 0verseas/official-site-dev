{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/console": "^1.0.2",
        "drupal/core": "^8.6.0",
        "drush/drush": "^9.0.0",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
	"wikimedia/composer-merge-plugin": "^1.4",
        "michelf/php-markdown": "^1.7",
        "drupal/video_embed_field": "^2.0",
        "drupal/media_entity": "^1.6",
        "drupal/back_to_top": "^1.0",
        "drupal/linkit": "^4.3",
        "drupal/editor_advanced_link": "^1.4",
        "drupal/ckeditor_font": "^1.0",
        "drupal/facebook_pixel": "^1.0",
        "drupal/asset_injector": "^2.2",
        "drupal/entity": "^1.0",
        "drupal/addtoany": "^1.10",
        "drupal/admin_toolbar": "^1.24",
        "drupal/ckeditor_markdown": "^1.1",
        "drupal/config_update": "^1.5",
        "drupal/contact_block": "^1.4",
        "drupal/domain": "1.x-dev",
        "drupal/domain_google_analytics": "^2.0",
        "drupal/features": "^3.8",
        "drupal/field_group": "^3.0",
        "drupal/editor_file": "^1.2",
        "drupal/inline_entity_form": "^1.0",
        "drupal/markdown": "^1.2",
        "drupal/views_infinite_scroll": "^1.5",
        "drupal/social_media_links": "^2.6",
        "drupal/smtp": "^1.0",
        "drupal/simplenews": "^1.0",
        "drupal/tvi": "^1.0",
        "drupal/seo_checklist": "^4.1",
        "drupal/amp": "^2.1"
    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "^8.6.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    }
}
